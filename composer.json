{
    "name": "eab/fancy-gallery-bundle",
    "description": "A simple gallery bundle for eZ Publish, using FancyBox",
    "authors": [
        {
            "name": "Andy Caiger",
            "email": "acaiger@eab.uk"
        }
    ],
    "require": {
        "ezsystems/ezpublish-kernel": ">=5.4|>=2014.11",
        "sp/bower-bundle": "@stable"
    },
    "autoload": {
        "psr-4": { "Eab\\FancyGalleryBundle\\": "" }
    },
    "minimum-stability": "stable",
    "scripts": {
       "post-install-cmd": [
           "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
       ],
       "post-update-cmd": [
           "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
       ]
   }
}
